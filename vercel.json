{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/region_settlement_data.json",
      "dest": "/region_settlement_data.json"
    },
    {
      "src": "/settlement_prediction_model.json",
      "dest": "/settlement_prediction_model.json"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
